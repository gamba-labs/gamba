{
  "name": "gamba-react-v2",
  "private": false,
  "version": "1.2.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
  "dev":   "tsup src/index.ts --watch --format cjs,esm --dts --external react --external @solana/web3.js --external @solana/spl-token --external @coral-xyz/anchor --external @solana/wallet-adapter-react --external @preact/signals-react --external gamba-core-v2 --external @gamba-labs/multiplayer-sdk",
  "build": "tsup src/index.ts --format cjs,esm --dts --external react --external @solana/web3.js --external @solana/spl-token --external @coral-xyz/anchor --external @solana/wallet-adapter-react --external @preact/signals-react --external gamba-core-v2 --external @gamba-labs/multiplayer-sdk",
  "lint":  "tsc",
  "clean": "rm -rf .turbo node_modules dist"
},
  "dependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@preact/signals-react": "^1.3.8",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.93.0",
    "gamba-core-v2": "workspace:*",
    "react": "^18.3.1",
    "swr": "^2.2.4",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/react": "^18.2.13",
    "@solana/wallet-adapter-react": "^0.15.35",
    "assert": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.93.0",
    "gamba-core-v2": "*",
    "react": "^18.3.1"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-react": {
      "optional": true
    }
  }
}
